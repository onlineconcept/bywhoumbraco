@inject ResourceHelper rh
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"]</title>
    <meta name="description" content="@ViewData["description"]" />
    <meta name="keywords" content="@ViewData["keywords"]" />
    <meta property="og:title" content="@ViewData["Title"]">
    <meta property="og:site_name" content="By Who">
    <meta property="og:url" content="@("https://"+ Context.Request.Host.Value + Context.Request.Path)">
    <meta property="og:description" content="@ViewData["description"]">
    <meta property="og:type" content="@ViewData["contentType"]">
    <meta property="og:image" content="@ViewData["image"]">
    <environment include="Development">
        <link href='https://fonts.googleapis.com/css?family=Roboto:100,300,400,500,700,900|Material+Icons' rel="stylesheet">
        <link href="~/css/styles.css" rel="stylesheet" />
    </environment>
    <environment exclude="Development">
        <link href='https://fonts.googleapis.com/css?family=Roboto:100,300,400,500,700,900|Material+Icons' rel="stylesheet">
        <link href="~/css/styles.css" rel="stylesheet" />
    </environment>
</head>
<body>
    <div oc-app  v-cloak :labels="@(await rh.GetAllResources())">
        <page-wrap :checkout="true" >
            @RenderBody()
        </page-wrap>
    </div>

    <environment include="Development">


        <script src="~/js/bundle.js"></script>

    </environment>
    <environment exclude="Development">


        <script src="~/js/bundle.js"></script>
    </environment>

    @RenderSection("Scripts", required: false)
</body>
</html>